<?xml version="1.0" encoding="UTF-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="Openbox 3">
    <item label="Terminal emulator">
      <action name="Execute">
        <command>
          x-terminal-emulator
        </command>
      </action>
    </item>
    <item label="Web browser">
      <action name="Execute">
        <command>
          x-www-browser
        </command>
      </action>
    </item>
    <separator/>
    <menu id="file-mangers" label="File Manager">
      <item label="Thunar">
        <action name="Execute">
          <command>
            thunar
          </command>
        </action>
      </item>
      <item label="SpaceFM">
        <action name="Execute">
          <command>
            spacefm
          </command>
        </action>
      </item>
    </menu>
    <menu id="favorites" label="Favorites">
      <item label="Downloads">
        <action name="Execute">
          <command>
              thunar ~/downloads
          </command>
        </action>
      </item>
    </menu>
    <menu execute="cb-places-pipemenu" id="pipe-placesmenu" label="Places"/>
    <separator/>
    <menu id="accessories" label="Accessories">
      <item label="Catfish File Search">
        <action name="Execute">
          <command>
            catfish
          </command>
        </action>
      </item>
      <item label="Archive Manager">
        <action name="Execute">
          <command>xarchiver</command>
        </action>
      </item>
      <item label="Task Manager (htop)">
        <action name="Execute">
          <command>
            xfce4-terminal --maximize --command="htop"
          </command>
        </action>
      </item>
      <item label="Thunar File Manager (root)">
        <action name="Execute">
          <command>
            gksudo thunar
          </command>
        </action>
      </item>
    </menu>
    <menu id="graphics" label="Graphics">
      <item label="GIMP">
        <action name="Execute">
          <command>
            gimp
          </command>
        </action>
      </item>
      <item label="Viewnior Image Viewer">
        <action name="Execute">
          <command>
            viewnior
          </command>
        </action>
      </item>
      <menu id="graphicsScreenshots" label="Take Screenshot">
        <item label="Screenshooter">
          <action name="Execute">
            <command>
              xfce4-screenshooter
            </command>
          </action>
        </item>
        <separator label="scrot"/>
        <item label="Now">
          <action name="Execute">
            <command>
              scrot '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/images/ &amp; viewnior ~/images/$f'
            </command>
          </action>
        </item>
        <item label="In 5 Seconds...">
          <action name="Execute">
            <command>
              scrot -d 5 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/images/ &amp; viewnior ~/images/$f'
            </command>
          </action>
        </item>
        <item label="In 10 Seconds...">
          <action name="Execute">
            <command>
              scrot -d 10 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/images/ &amp; viewnior ~/images/$f'
            </command>
          </action>
        </item>
        <item label="Selected Area... (click &amp; drag mouse)">
          <action name="Execute">
            <command>
              scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/images/ &amp; viewnior ~/images/$f'
            </command>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="multimedia" label="Multimedia">
      <item label="VLC Media Player">
        <action name="Execute">
          <command>
            vlc
          </command>
        </action>
      </item>
      <item label="Volume Control">
        <action name="Execute">
          <command>
            pavucontrol
          </command>
        </action>
      </item>
    </menu>
    <menu execute="cb-sshconfig-pipemenu" id="sshconfig" label="SSH"/>
    <separator/>
    <!-- This requires the presence of the 'menu' package to work -->
    <menu id="/Debian"/>
    <separator/>
    <menu id="settings" label="Settings">
      <item label="Edit .vimrc">
        <action name="Execute">
          <command>xfce4-terminal --maximize --command="vim ~/.vimrc"</command>
        </action>
      </item>
      <menu id="bash-settings" label="Bash">
        <item label="Edit .bashrc">
          <action name="Execute">
            <command>xfce4-terminal --maximize --command="vim ~/.bashrc"</command>
          </action>
        </item>
        <item label="Edit .inputrc">
          <action name="Execute">
            <command>xfce4-terminal --maximize --command="vim ~/.inputrc"</command>
          </action>
        </item>
      </menu>
      <menu id="obConfig" label="Openbox">
        <item label="Edit menu.xml">
          <action name="Execute">
            <command>
              xfce4-terminal --maximize --command="vim ~/.config/openbox/menu.xml"
            </command>
          </action>
        </item>
        <item label="Edit rc.xml">
          <action name="Execute">
            <command>
              xfce4-terminal --maximize --command="vim ~/.config/openbox/rc.xml"
            </command>
          </action>
        </item>
        <item label="Edit autostart">
          <action name="Execute">
            <command>
              xfce4-terminal --maximize --command="vim ~/.config/openbox/autostart"
            </command>
          </action>
        </item>
        <separator/>
        <item label="GUI Menu Editor">
          <action name="Execute">
            <command>
              obmenu
            </command>
          </action>
        </item>
        <item label="GUI Applications Settings Editor">
          <action name="Execute">
            <command>
              obapps
            </command>
          </action>
        </item>
        <item label="GUI Config Tool">
          <action name="Execute">
            <command>
              obconf
            </command>
          </action>
        </item>
        <separator/>
        <item label="Reconfigure">
          <action name="Reconfigure"/>
        </item>
        <item label="Restart">
          <action name="Restart"/>
        </item>
      </menu>
      <menu id="tint2config" label="tint2">
        <item label="Edit config file">
          <action name="Execute">
            <command>
              xfce4-terminal --maximize --command="vim ~/.config/tint2/tint2rc"
            </command>
          </action>
        </item>
        <item label="Restart tint2">
          <action name="Execute">
            <command>
              tint2restart
            </command>
          </action>
        </item>
      </menu>
      <item label="ARandR">
        <action name="Execute">
          <command>
            arandr
          </command>
        </action>
      </item>
      <item label="Notifications">
        <action name="Execute">
          <command>xfce4-notifyd-config</command>
        </action>
      </item>
      <item label="Default Applications">
        <action name="Execute">
          <command>
            xfce4-terminal --maximize --command="sudo update-alternatives --all"
          </command>
        </action>
      </item>
      <item label="Power Management">
        <action name="Execute">
          <command>
            mate-power-preferences
          </command>
        </action>
      </item>
      <item label="Screensaver">
        <action name="Execute">
          <command>
            xscreensaver-demo
          </command>
        </action>
      </item>
      <item label="Choose Wallpaper">
        <action name="Execute">
          <command>
            nitrogen ~/images/wallpapers/
          </command>
        </action>
      </item>
      <item label="GParted">
        <action name="Execute">
          <command>
            gksudo gparted
          </command>
        </action>
      </item>
      <item label="Synaptic Package Manager">
        <action name="Execute">
          <command>
            gksudo synaptic
          </command>
        </action>
      </item>
    </menu>
    <separator/>
    <menu id="exit" label="Exit">
      <item label="Lock">
        <action name="Execute">
          <command>
            xscreensaver-command -lock
          </command>
        </action>
      </item>
      <item label="Suspend">
        <action name="Execute">
          <command>
            systemctl suspend
          </command>
        </action>
      </item>
      <item label="Log Out">
        <action name="Execute">
          <command>
            openbox --exit
          </command>
        </action>
      </item>
      <item label="Reboot">
        <action name="Execute">
          <command>
            systemctl reboot
          </command>
        </action>
      </item>
      <item label="Shut Down">
        <action name="Execute">
          <command>
            systemctl poweroff
          </command>
        </action>
      </item>
    </menu>
  </menu>
</openbox_menu>
