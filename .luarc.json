{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "runtime.version": "Lua 5.4",
    "diagnostics.globals": [
        "vim",
        "use",
        "packer_plugins",
        "require",
        "pcall",
        "pairs",
        "ipairs"
    ],
    "format.defaultConfig": {
        "call_arg_parentheses": "remove",
        "quote_style": "single",
        "indent_style": "space",
        "indent_size": "2"
    }
}
